{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "SETUPTOOLS_USE_DISTUTILS": "stdlib"
  },
  "functions": {
    "api/app.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}